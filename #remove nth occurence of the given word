

value1 = int(input("enter the integer "))
b = []
for i in range(0,value1):
    temp = input()
    b.append(temp)
    print(b)
    
word = input("enter the string")

n = int(input("enter the integer"))

count = 0

for i in range(0,len(b)):
    if(b[i]==word):
        count = count + 1
        if(count==n):
            del b[i]
print("updated list is",b)
